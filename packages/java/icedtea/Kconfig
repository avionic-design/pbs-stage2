config PACKAGE_JAVA_ICEDTEA
	bool "icedtea"
	select PACKAGE_JAVA_ICEDTEA_BUILD
	select PACKAGE_LIBS_CUPS
	select PACKAGE_LIBS_FREETYPE
	select PACKAGE_LIBS_LIBJPEG
	select PACKAGE_LIBS_LIBPNG
	select PACKAGE_XORG_PROTO_X
	select PACKAGE_LIBS_LIBGIF
	select PACKAGE_LIBS_ZLIB
	select PACKAGE_MULTIMEDIA_ALSA_CORE
	select PACKAGE_XORG_LIB_XINERAMA
	select PACKAGE_XORG_LIB_XP
	select PACKAGE_XORG_LIB_XRENDER
	select PACKAGE_XORG_LIB_XT
	select PACKAGE_XORG_LIB_XTST
	default n

config PACKAGE_JAVA_ICEDTEA_PLUGIN
	bool "plugin"
	depends on PACKAGE_JAVA_ICEDTEA
	select PACKAGE_LIBS_GTK
	#select PACKAGE_LIBS_LIBFFI
	select PACKAGE_WEB_MOZILLA_XULRUNNER
	default n
