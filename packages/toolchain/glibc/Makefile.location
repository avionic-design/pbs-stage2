PACKAGE = glibc
VERSION = 2.16.0
LICENSE = LGPL

LOCATION = http://ftp.gnu.org/pub/gnu/$(PACKAGE)
TARBALLS = \
	$(PACKAGE)-$(VERSION).tar.xz \
	$(PACKAGE)-ports-$(VERSION).tar.xz

MAJOR = $(word 1,$(subst ., ,$(VERSION)))
