config PACKAGE_TOOLCHAIN_GCC_CORE
	bool "bootstrap GCC"
	select PACKAGE_TOOLCHAIN_BINUTILS
	select PACKAGE_TOOLCHAIN_GLIBC_HEADERS if GLIBC
	select PACKAGE_TOOLCHAIN_UCLIBC_HEADERS if UCLIBC
	select PACKAGE_TOOLCHAIN_NEWLIB_HEADERS if OS_NONE
	select PACKAGE_TOOLCHAIN_GMP
	select PACKAGE_TOOLCHAIN_MPFR
	select PACKAGE_TOOLCHAIN_MPC
	default n
