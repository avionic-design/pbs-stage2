menu "World-Wide Web Utilities"

config PACKAGE_WEB_WEBKIT
	bool "webkit"
	select VIRTUAL_DEVEL_LIBC
	default n

config PACKAGE_WEB_WEBKIT_GTK
	bool "webkit-gtk"
	select PACKAGE_LIBS_ENCHANT
	select PACKAGE_LIBS_GTK
	select PACKAGE_LIBS_ICU
	select PACKAGE_LIBS_LIBSOUP
	select PACKAGE_LIBS_LIBXML2
	select PACKAGE_LIBS_LIBXSLT
	select PACKAGE_LIBS_SQLITE
	default n

endmenu
