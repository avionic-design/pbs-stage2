+    exit 1
+fi
+
+file=$1
+
+function subst () {
+    from=$1
+    to=$2
+    if grep -q $from $file; then
+        echo "$file: $from->$to"
+        perl -i -p -e "s/$from/$to/g" $file
+    fi
