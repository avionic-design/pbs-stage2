+  if (GST_MESSAGE_TYPE (message) == GST_MESSAGE_ERROR) {
+    fail_unless (gerror == NULL && error_debug == NULL);
+    fail_unless (GST_MESSAGE_SRC (message) == GST_OBJECT (demux));
