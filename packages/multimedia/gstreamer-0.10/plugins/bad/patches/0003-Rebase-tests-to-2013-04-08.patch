+  printf("\nDumping %s (size = %u)\n\n", tag, size);
+
+  for(i=0; i<size; ++i) {
+
+    if (i % 16 == 0)
+      printf("%04x:%04x | ", i >> 16, i & 0xFFFF);
+
+    printf("%02x", buf[i]);
+
+    if (i % 16 != 15)
+      printf(" ");
+    else
+      printf("\n");
+
+  }
+
+  printf("\n\n");
+
+}
