config PACKAGE_MULTIMEDIA_GSTREAMER_PLUGINS_FFMPEG
	bool "ffmpeg"
	select PACKAGE_LIBS_ORC if PACKAGE_MULTIMEDIA_GSTREAMER_PLUGINS_FFMPEG_ORC
	select PACKAGE_MULTIMEDIA_GSTREAMER_CORE
	select PACKAGE_MULTIMEDIA_GSTREAMER_PLUGINS_BASE
	select PACKAGE_MULTIMEDIA_FFMPEG
	default n

config PACKAGE_MULTIMEDIA_GSTREAMER_PLUGINS_FFMPEG_ORC
	bool "ORC support"
	depends on PACKAGE_MULTIMEDIA_GSTREAMER_PLUGINS_FFMPEG
	default n
