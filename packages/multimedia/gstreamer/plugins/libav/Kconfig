config PACKAGE_MULTIMEDIA_GSTREAMER_PLUGINS_LIBAV
	bool "libav"
	select PACKAGE_LIBS_ORC if PACKAGE_MULTIMEDIA_GSTREAMER_PLUGINS_LIBAV_ORC
	select PACKAGE_MULTIMEDIA_GSTREAMER_CORE
	select PACKAGE_MULTIMEDIA_GSTREAMER_PLUGINS_BASE
	select PACKAGE_MULTIMEDIA_LIBAV
	default n

config PACKAGE_MULTIMEDIA_GSTREAMER_PLUGINS_LIBAV_ORC
	bool "ORC support"
	depends on PACKAGE_MULTIMEDIA_GSTREAMER_PLUGINS_LIBAV
	default n
