	default n
	select VIRTUAL_KERNEL_LINUX_LIBC
	select PACKAGE_DEVEL_GLIBC if GLIBC
	select PACKAGE_DEVEL_UCLIBC if UCLIBC

source "packages/devel/flex/Kconfig"
