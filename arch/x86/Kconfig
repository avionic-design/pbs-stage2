config ARCH
	string
	default "x86"

#
# x86 CPU selection
#
choice
	prompt "CPU"

config ARCH_X86_GENERIC
	bool "generic"

config ARCH_X86_I686
	bool "i686"

config ARCH_X86_I786
	bool "i786"

endchoice

if ARCH_X86_GENERIC
source "arch/x86/generic/Kconfig"
endif

if ARCH_X86_I686
source "arch/x86/i686/Kconfig"
endif

if ARCH_X86_I786
source "arch/x86/i786/Kconfig"
endif
