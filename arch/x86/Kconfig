if ARCH_X86

config ARCH
	string
	default "x86"

#
# x86 CPU selection
#
choice
	prompt "CPU"

config ARCH_X86_I386
	bool "i386"

config ARCH_X86_I686
	bool "i686"

config ARCH_X86_I786
	bool "i786"

endchoice

source "arch/x86/i386/Kconfig"
source "arch/x86/i686/Kconfig"
source "arch/x86/i786/Kconfig"

endif
